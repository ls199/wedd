@model IEnumerable<WebApplication1.Models.Employee>
@{
    ViewBag.Title = "Search";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Search</title>
</head>
<body>
   <table>
       <tr class="table">
           <th>姓名</th>
           <th>工资</th>
       </tr>
       @foreach (var item in Model)
       {
    <tr>
        <th>@item.Name</th>
        <th>@item.Salary</th>
    </tr>
      }
      
   </table>
</body>
</html>
